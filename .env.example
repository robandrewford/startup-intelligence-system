# Database Configuration
DB_HOST=localhost
DB_PORT=5432
DB_NAME=startup_intelligence
DB_USER=postgres
DB_PASSWORD=your_password_here

# API Keys (Optional but recommended)
GITHUB_TOKEN=your_github_token_here
CRUNCHBASE_API_KEY=your_crunchbase_key_here
HUGGINGFACE_TOKEN=your_huggingface_token_here
SENDGRID_API_KEY=your_sendgrid_key_here

# LinkedIn (Use with caution - requires careful rate limiting)
LINKEDIN_EMAIL=your_email@example.com
LINKEDIN_PASSWORD=your_password_here

# Y Combinator (If API becomes available)
YC_API_KEY=your_yc_key_here

# Application Settings
LOG_LEVEL=INFO
RATE_LIMIT_DELAY=2.0
MAX_RETRIES=3
REQUEST_TIMEOUT=30

# Target Criteria (Override defaults)
MIN_AI_WASHING_SCORE=6
MIN_STRATEGIC_FIT_SCORE=6
MIN_EMPLOYEES=10
MAX_EMPLOYEES=200
HEALTHCARE_FOCUS=true

# Discovery Sources (Enable/disable)
ENABLE_YC_SCRAPING=true
ENABLE_GEEKWIRE_SCRAPING=true
ENABLE_LINKEDIN_SCRAPING=false
ENABLE_GITHUB_ANALYSIS=true
ENABLE_CRUNCHBASE_LOOKUP=false
